    "num=int(input())\n",
    "\n",
    "lis=[]\n",
    "def hanoi(num,a,b,c):\n",
    "    global lis\n",
    "    \n",
    "    if num==1:\n",
    "        lis.append((a,c))\n",
    "    else:\n",
    "        hanoi(num-1,a,c,b)\n",
    "        lis.append((a,c))\n",
    "        hanoi(num-1,b,a,c)\n",
    "    \n",
    "hanoi(num,1,2,3)\n",
    "print(len(lis))\n",
    "for x,y in lis:\n",
    "    print(x,y)"
   ]
